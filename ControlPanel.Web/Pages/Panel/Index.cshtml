@page

@model ControlPanel.Web.Pages.Panel.IndexModel

@{
    ViewData["Title"] = "Home page";
}

<head>
    <link href="~/css/Panel.css" rel="stylesheet" />
</head>

<div class="calculator">

    <div class="calc-row">
        <div class="screen"></div>
    </div>


    <div class="calc-row">
        <div class="button">1</div><div class="button">2</div><div class="button">3</div>@*<div class="button">-</div><div class="button pi">pi</div>*@
    </div>

    <div class="calc-row">
        <div class="button" data-Status="Ideal">4</div><div class="button">5</div><div class="button">6</div>@*<div class="button multiply">*</div><div class="button inverse">1/x</div>*@
    </div>

    <div class="calc-row">
        <div class="button" data-Status="Ideal">7</div><div class="button">8</div><div class="button nine" id="bt9">9</div>@*<div class="button divice">/</div><div class="button root">sqrt</div>*@
    </div>



    <div class="calc-row">
        <div class="button zero" id="bt0">0</div>@*<div class="button decimal">.</div><div class="button">+</div><div class="button equal">=</div>*@
    </div>
</div>

<div class="row">
    <div class="col-6">
        <ul id="messagesList">List</ul>
    </div>
</div>

@section Scripts
{

    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/Calc/Panel.js"></script>
}